call pathogen#incubate()
call pathogen#infect()
call pathogen#helptags()
syntax on
set hlsearch "highlight search
set number "line number
set mouse=a "mouse clickable
colors jellybeans
set expandtab
set tabstop=4
set shiftwidth=4
set smartindent
nmap <C-e> :e#<CR>
set clipboard=unnamedplus
autocmd vimenter * NERDTree
